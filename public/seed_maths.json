{
  "calculation_methods": {
    "basic_arithmetic": {
      "addition": {
        "algorithm": "Add digits column by column from right to left, carry over when sum > 9",
        "steps": [
          "1. Align numbers by place value (ones, tens, hundreds)",
          "2. Start from rightmost column (ones place)",
          "3. Add digits in column, if sum ≥ 10, write ones digit and carry tens digit",
          "4. Move left to next column, add digits plus any carry",
          "5. Repeat until all columns complete"
        ],
        "examples": {
          "47_plus_38": {
            "setup": "  47\n+ 38\n----",
            "step1": "7 + 8 = 15, write 5 carry 1",
            "step2": "4 + 3 + 1(carry) = 8",
            "result": "85",
            "verification": "47 + 38 = 85"
          },
          "10_plus_10": {
            "setup": "  10\n+ 10\n----",
            "step1": "0 + 0 = 0",
            "step2": "1 + 1 = 2",
            "result": "20",
            "verification": "10 + 10 = 20"
          }
        }
      },
      "subtraction": {
        "algorithm": "Subtract digits column by column, borrow when needed",
        "steps": [
          "1. Align numbers by place value",
          "2. Start from rightmost column",
          "3. If top digit < bottom digit, borrow from left column",
          "4. Subtract bottom from top digit",
          "5. Move left, repeat process"
        ],
        "examples": {
          "85_minus_47": {
            "setup": "  85\n- 47\n----",
            "step1": "5 - 7: need to borrow, 15 - 7 = 8",
            "step2": "7 - 4 = 3 (after borrowing)",
            "result": "38"
          }
        }
      },
      "multiplication": {
        "algorithm": "Multiply each digit by multiplier, add partial products",
        "steps": [
          "1. Write numbers with larger on top",
          "2. Multiply top number by each digit of bottom number",
          "3. Shift each partial product one place left",
          "4. Add all partial products"
        ],
        "examples": {
          "10_times_10": {
            "setup": "  10\n× 10\n----",
            "step1": "10 × 0 = 0",
            "step2": "10 × 1 = 10 (shift left: 100)",
            "step3": "0 + 100 = 100",
            "result": "100",
            "verification": "10 × 10 = 100"
          },
          "23_times_45": {
            "setup": "   23\n×  45\n-----",
            "step1": "23 × 5 = 115",
            "step2": "23 × 4 = 92 (shift left: 920)",
            "step3": "115 + 920 = 1035",
            "result": "1035"
          }
        }
      },
      "division": {
        "algorithm": "Long division: divide, multiply, subtract, bring down",
        "steps": [
          "1. How many times does divisor go into first digits of dividend?",
          "2. Multiply divisor by that quotient digit",
          "3. Subtract result from dividend section",
          "4. Bring down next digit",
          "5. Repeat until complete"
        ],
        "examples": {
          "100_divided_by_10": {
            "setup": "100 ÷ 10",
            "step1": "10 goes into 10 once (1), 1×10=10",
            "step2": "10-10=0, bring down 0 to make 00",
            "step3": "10 goes into 0 zero times (0)",
            "result": "10",
            "verification": "100 ÷ 10 = 10"
          },
          "144_divided_by_12": {
            "setup": "144 ÷ 12",
            "step1": "12 goes into 14 once (1), 1×12=12",
            "step2": "14-12=2, bring down 4 to make 24",
            "step3": "12 goes into 24 twice (2), 2×12=24",
            "step4": "24-24=0",
            "result": "12"
          }
        }
      }
    },
